{
  "name": "harmonized-darkhc-vscode-theme",
  "displayName": "Harmonized Dark",
  "description": "Dark Solarized in harmony",
  "version": "0.1.1",
  "scripts": {
    "release:major": "vsce publish major && git push",
    "release:minor": "vsce publish minor && git push",
    "release:patch": "vsce publish patch && git push",
    "build": "node src/index.js"
  },
  "preview": false,
  "publisher": "darlissonlimeira",
  "galleryBanner": {
    "color": "#fdf6e3",
    "theme": "light"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/darlissonlimeira/harmonized-vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/darlissonlimeira/harmonized-vscode-theme/issues"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "solarized",
    "light",
    "dark",
    "zen",
    "contrast"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Harmonized Dark HC",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-hc.json"
      }
    ]
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.82.0"
  },
  "devDependencies": {
    "@types/vscode": "1.82.0",
    "node-fetch": "^2.6.7",
    "strip-json-comments": "^3.1.1",
    "vsce": "^2.15.0"
  }
}
